{
  "compilerOptions": {
    "target": "es2020",
    "jsx": "preserve",
    "module": "esnext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "types": [
      "vitest/globals"
    ],
    "allowJs": true,
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noEmit": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "skipLibCheck": true
  }
}
